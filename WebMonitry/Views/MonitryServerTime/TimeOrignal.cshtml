            <label>Software Time</label>

            <input id="inputTime" type="text" class="form-control" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <label>Current Time</label>
            <input id="CurTime" type="text" class="form-control" readonly />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" id="change" value="Search" class="btn btn-default" />
        </div>
