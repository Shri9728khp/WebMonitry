@model List<WebMonitry.DataDbc.Monotringin>
<h2>*****Server Time ******</h2>
<table class="table table-bordered label-info">
    <tr>

        <th>Id</th>
        <th>Server_Name</th>
        <th>Software</th>
        <th>Software1</th>
        <th>Software2</th>
        <th>Software3</th>
        <th>Software4</th>
        <th>Software5</th>
        <th>Software6</th>
        <th>Software7</th>
        <th>Software8</th>
        <th>Software9</th>
    </tr>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Server_Name</td>
                <td>@item.Software</td>
                <td>@item.Software1</td>
                <td>@item.Software2</td>
                <td>@item.Software3</td>
                <td>@item.Software4</td>
                <td>@item.Software5</td>
                <td>@item.Software6</td>
                <td>@item.Software7</td>
                <td>@item.Software8</td>
                <td>@item.Software9</td>
            </tr>
        }
    </tbody>
</table>
<br />
<div class="form-horizontal">
    <div class="form-group">
        <div class="col-md-10">
            <label>Enter Software</label>
            <select id="selectSoftware" class="form-control">
                <option disabled>----------------Select SoftWare---------------</option>
                <option>Software1</option>
                <option>Software2</option>
